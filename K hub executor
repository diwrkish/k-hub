local function d()print("Debug: Centering CircleButton")if G then local a=G:GetScreenResolution()or Vector2.new(1280,720)local b=k.Size.X.Offset k.Position=UDim2.new(0,(a.X-b)/2,0,(a.Y-b)/2)end end
d()

local function e(a)if a then print("Debug: Attaching drag to "..tostring(a)) local dragging,dragInput,dragStart,startPos=false,nil,nil,nil a.InputBegan:Connect(function(input)if input.UserInputType==Enum.UserInputType.MouseButton1 or input.UserInputType==Enum.UserInputType.Touch then dragging=true dragStart=input.Position startPos=a.Position input.Changed:Connect(function()if input.UserInputState==Enum.UserInputState.End then dragging=false end end)end end)a.InputChanged:Connect(function(input)if input.UserInputType==Enum.UserInputType.MouseMovement or input.UserInputType==Enum.UserInputType.Touch then dragInput=input end end)U.InputChanged:Connect(function(input)if input==dragInput and dragging then if input.UserInputType==Enum.UserInputType.MouseMovement or input.UserInputType==Enum.UserInputType.Touch then local delta=input.Position - dragStart a.Position=UDim2.new(startPos.X.Scale,startPos.X.Offset + delta.X,startPos.Y.Scale,startPos.Y.Offset + delta.Y)print("Debug: Dragging to "..tostring(a.Position))end end end)else print("Error: Nil frame for drag")end end
e(b)e(k)

local n=0 local o=0.3
k.MouseButton1Click:Connect(function()print("Debug: CircleButton clicked")local a=tick()if a-n<o then b.Visible=true k.Visible=false if T then T:Create(b,TweenInfo.new(0.3),{BackgroundTransparency=0.1}):Play()end end n=a end)
k.TouchTap:Connect(function()print("Debug: CircleButton touched")local a=tick()if a-n<o then b.Visible=true k.Visible=false if T then T:Create(b,TweenInfo.new(0.3),{BackgroundTransparency=0.1}):Play()end end n=a end)

local function p(a)print("Debug: Executing script")local s,err=pcall(function()loadstring(a)()end)if not s then print("Execution error: "..tostring(err))end end

i.MouseButton1Click:Connect(function()print("Debug: Execute clicked")p(g.Text)end)
j.MouseButton1Click:Connect(function()print("Debug: Clear clicked")g.Text="-- Enter your Lua script here"end)
f.MouseButton1Click:Connect(function()print("Debug: Minimize clicked")b.Visible=false k.Visible=true d()if T then T:Create(k,TweenInfo.new(0.3),{BackgroundTransparency=0.3,Size=UDim2.new(0,60,0,60)}):Play()end end)

local function q(a)if a and T then print("Debug: Attaching hover to "..tostring(a)) a.MouseEnter:Connect(function()T:Create(a,TweenInfo.new(0.2),{BackgroundColor3=Color3.fromRGB(a.BackgroundColor3.r*255+20,a.BackgroundColor3.g*255+20,a.BackgroundColor3.b*255+20),BackgroundTransparency=0}):Play()end)a.MouseLeave:Connect(function()T:Create(a,TweenInfo.new(0.2),{BackgroundColor3=Color3.fromRGB(a.BackgroundColor3.r*255-20,a.BackgroundColor3.g*255-20,a.BackgroundColor3.b*255-20),BackgroundTransparency=0.2}):Play()end)a.TouchTap:Connect(function()T:Create(a,TweenInfo.new(0.1),{BackgroundColor3=Color3.fromRGB(a.BackgroundColor3.r*255+20,a.BackgroundColor3.g*255+20,a.BackgroundColor3.b*255+20),BackgroundTransparency=0}):Play()wait(0.1)T:Create(a,TweenInfo.new(0.1),{BackgroundColor3=Color3.fromRGB(a.BackgroundColor3.r*255-20,a.BackgroundColor3.g*255-20,a.BackgroundColor3.b*255-20),BackgroundTransparency=0.2}):Play()end)else print("Error: Nil button for hover")end end
q(i)q(j)q(f)q(k)

local function r(a)print("[K Hub Executor Error]: "..a)end

local function s()local a=getrawmetatable(game)if a then local b=a.__index setrawmetatable(game,{__index=function(a,c)if c=="GetService"then return function(a,d)r("Service access detected: "..d)return b(a,d)end end return b(a,c)end})else print("Error: Metatable not found")end end
s()


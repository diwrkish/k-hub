local function tweenButton(b)
    local os = b.Size
    local ti = TweenInfo.new(0.1,Enum.EasingStyle.Quad,Enum.EasingDirection.Out)
    local su = TS:Create(b,ti,{Size=os+UDim2.new(0,5,0,5)})
    local sd = TS:Create(b,ti,{Size=os})
    su:Play()
    su.Completed:Connect(function() sd:Play() end)
end
local function ag()
    local ti = TweenInfo.new(0.2,Enum.EasingStyle.Back,Enum.EasingDirection.Out)
    local su = TS:Create(GB,ti,{Size=UDim2.new(0.85,0,0,45)})
    local sd = TS:Create(GB,ti,{Size=UDim2.new(0.9,0,0,50)})
    su:Play()
    su.Completed:Connect(function() sd:Play() end)
end
local function asf()
    local ti = TweenInfo.new(0.2,Enum.EasingStyle.Back,Enum.EasingDirection.Out)
    local su = TS:Create(SFB,ti,{Size=UDim2.new(0.85,0,0,45)})
    local sd = TS:Create(SFB,ti,{Size=UDim2.new(0.9,0,0,50)})
    su:Play()
    su.Completed:Connect(function() sd:Play() end)
end
local function setFarmSlap()
    local ti = TweenInfo.new(0.2,Enum.EasingStyle.Back,Enum.EasingDirection.Out)
    local su = TS:Create(FarmSB,ti,{Size=UDim2.new(0.85,0,0,45)})
    local sd = TS:Create(FarmSB,ti,{Size=UDim2.new(0.9,0,0,50)})
    su:Play()
    su.Completed:Connect(function() sd:Play() end)
end
local SFR = false
local SFC = nil
local CurrentFarmSlap = "DefaultSlap"
local function sfl()
    while SFR do
        local s,e = pcall(function()
            if not LP.Character then return end
            local se = LP.Character:WaitForChild(CurrentFarmSlap):WaitForChild("Event")
            for i,v in pairs(Players:GetPlayers()) do
                if v~=LP and v.Character then
                    local a = {"slash",v.Character,Vector3.new(-8.914801597595215,2.4258898179141397e-07,4.53059720993042)}
                    se:FireServer(unpack(a))
                end
            end
        end)
        if not s then
            SL.Text = "‚ùå Slap Farm Error: "..tostring(e)
            SL.TextColor3 = Color3.fromRGB(255,100,100)
            SFR = false
            SFB.Text = "üåæ START SLAP FARM"
            SFB.BackgroundColor3 = Color3.fromRGB(200,50,50)
            SFBG.Color = ColorSequence.new({ColorSequenceKeypoint.new(0,Color3.fromRGB(220,60,60)),ColorSequenceKeypoint.new(1,Color3.fromRGB(180,40,40))})
        end
        task.wait()
    end
end
local function tsf()
    SFR = not SFR
    if SFR then
        SFB.Text = "üõë STOP SLAP FARM"
        SFB.BackgroundColor3 = Color3.fromRGB(50,200,50)
        SFBG.Color = ColorSequence.new({ColorSequenceKeypoint.new(0,Color3.fromRGB(60,220,60)),ColorSequenceKeypoint.new(1,Color3.fromRGB(40,180,40))})
        SL.Text = "üåæ Slap Farm running with "..CurrentFarmSlap
        SL.TextColor3 = Color3.fromRGB(100,255,100)
        SFC = coroutine.create(sfl)
        coroutine.resume(SFC)
    else
        SFB.Text = "üåæ START SLAP FARM"
        SFB.BackgroundColor3 = Color3.fromRGB(200,50,50)
        SFBG.Color = ColorSequence.new({ColorSequenceKeypoint.new(0,Color3.fromRGB(220,60,60)),ColorSequenceKeypoint.new(1,Color3.fromRGB(180,40,40))})
        SL.Text = "üåæ Slap Farm stopped"
        SL.TextColor3 = Color3.fromRGB(150,150,170)
        SFR = false
    end
    asf()
end
local function gs()
    local sn = GI.Text
    if sn=="" then return end
    local a = {[1]=sn}
    local s,e = pcall(function() ESE:FireServer(unpack(a)) end)
    if s then
        SL.Text = "‚úÖ "..sn.." given!"
        SL.TextColor3 = Color3.fromRGB(100,255,100)
    else
        SL.Text = "‚ùå Error: "..tostring(e)
        SL.TextColor3 = Color3.fromRGB(255,100,100)
    end
    ag()
end
local function setFarmSlapType()
    local sn = FarmGI.Text
    if sn=="" then return end
    local a = {[1]=sn}
    local s,e = pcall(function() ESE:FireServer(unpack(a)) end)
    if s then
        CurrentFarmSlap = sn
        SL.Text = "‚úÖ Farm slap set to "..sn
        SL.TextColor3 = Color3.fromRGB(100,255,100)
    else
        SL.Text = "‚ùå Error: "..tostring(e)
        SL.TextColor3 = Color3.fromRGB(255,100,100)
    end
    setFarmSlap()
end
local function showSlapTab()
    SlapTab.Visible = true
    FarmTab.Visible = false
    SlapTabButton.BackgroundColor3 = Color3.fromRGB(50,50,60)
    FarmTabButton.BackgroundColor3 = Color3.fromRGB(40,40,50)
end
local function showFarmTab()
    SlapTab.Visible = false
    FarmTab.Visible = true
    SlapTabButton.BackgroundColor3 = Color3.fromRGB(40,40,50)
    FarmTabButton.BackgroundColor3 = Color3.fromRGB(50,50,60)
end
local IM = false
local OS = MF.Size
local MS = UDim2.new(0,200,0,60)
local function tm()
    IM = not IM
    if IM then
        local ti = TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out)
        local mt = TS:Create(MF,ti,{Size=MS,Position=UDim2.new(0.5,-100,0.5,-30)})
        for _,c in ipairs(MF:GetChildren()) do if c~=H and c:IsA("GuiObject") then c.Visible=false end end
        mt:Play()
        MB.Text = "+"
    else
        local ti = TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out)
        local mt = TS:Create(MF,ti,{Size=OS,Position=UDim2.new(0.5,-200,0.5,-250)})
        for _,c in ipairs(MF:GetChildren()) do if c:IsA("GuiObject") then c.Visible=true end end
        mt:Play()
        MB.Text = "_"
    end
end
local function cg()
    local ti = TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out)
    local ct = TS:Create(MF,ti,{Size=UDim2.new(0,0,0,0),Position=UDim2.new(0.5,0,0.5,0)})
    ct:Play()
    ct.Completed:Connect(function() SG:Destroy() end)
end
SlapTabButton.MouseButton1Click:Connect(showSlapTab)
FarmTabButton.MouseButton1Click:Connect(showFarmTab)
GB.MouseButton1Click:Connect(gs)
GI.FocusLost:Connect(function(ep) if ep then gs() end end)
FarmSB.MouseButton1Click:Connect(setFarmSlapType)
FarmGI.FocusLost:Connect(function(ep) if ep then setFarmSlapType() end end)
SFB.MouseButton1Click:Connect(tsf)
CB.MouseButton1Click:Connect(cg)
MB.MouseButton1Click:Connect(tm)
local function sbh(b,nc,hc)
    b.MouseEnter:Connect(function() b.BackgroundColor3=hc end)
    b.MouseLeave:Connect(function() b.BackgroundColor3=nc end)
end
sbh(CB,Color3.fromRGB(220,60,60),Color3.fromRGB(255,80,80))
sbh(MB,Color3.fromRGB(100,100,140),Color3.fromRGB(120,120,160))
sbh(SlapTabButton,Color3.fromRGB(40,40,50),Color3.fromRGB(60,60,70))
sbh(FarmTabButton,Color3.fromRGB(40,40,50),Color3.fromRGB(60,60,70))
GB.MouseEnter:Connect(function() GB.BackgroundColor3=Color3.fromRGB(0,170,255) end)
GB.MouseLeave:Connect(function() GB.BackgroundColor3=Color3.fromRGB(0,150,255) end)
FarmSB.MouseEnter:Connect(function() FarmSB.BackgroundColor3=Color3.fromRGB(0,170,255) end)
FarmSB.MouseLeave:Connect(function() FarmSB.BackgroundColor3=Color3.fromRGB(0,150,255) end)
SFB.MouseEnter:Connect(function() SFB.BackgroundColor3=SFR and Color3.fromRGB(60,220,60) or Color3.fromRGB(220,60,60) end)
SFB.MouseLeave:Connect(function() SFB.BackgroundColor3=SFR and Color3.fromRGB(50,200,50) or Color3.fromRGB(200,50,50) end)
local D = false
local DS = nil
local SP = nil
H.InputBegan:Connect(function(i) if i.UserInputType==Enum.UserInputType.MouseButton1 then D=true DS=i.Position SP=MF.Position end end)
H.InputChanged:Connect(function(i) if D and i.UserInputType==Enum.UserInputType.MouseMovement then local d=i.Position-DS MF.Position=UDim2.new(SP.X.Scale,SP.X.Offset+d.X,SP.Y.Scale,SP.Y.Offset+d.Y) end end)
H.InputEnded:Connect(function(i) if i.UserInputType==Enum.UserInputType.MouseButton1 then D=false end end)
UIS.InputBegan:Connect(function(i,gp) if i.KeyCode==Enum.KeyCode.C and not gp then dp("Activating Infinite Yield") loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))() end end)

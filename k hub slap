NC.CornerRadius = UDim.new(0,12)
NC.Parent = NF
local NS = Instance.new("UIStroke")
NS.Color = Color3.fromRGB(100,100,100)
NS.Thickness = 2
NS.Transparency = 0.5
NS.Parent = NF
local NSS = Instance.new("UIStroke")
NSS.Color = Color3.fromRGB(0,0,0)
NSS.Thickness = 4
NSS.Transparency = 0.7
NSS.Parent = NF
local NTL = Instance.new("TextLabel")
NTL.Size = UDim2.new(1,0,1,0)
NTL.BackgroundTransparency = 1
NTL.Text = "To activate Inf Yield, tap C"
NTL.TextColor3 = Color3.fromRGB(220,220,220)
NTL.TextScaled = true
NTL.Font = Enum.Font.GothamBold
NTL.TextSize = 24
NTL.Parent = NF
local function fadeOutNG()
    dp("Starting notification fade-out")
    local ti = TweenInfo.new(1,Enum.EasingStyle.Sine,Enum.EasingDirection.InOut)
    local ft = TS:Create(NF,ti,{BackgroundTransparency=1})
    local tt = TS:Create(NTL,ti,{TextTransparency=1})
    local st = TS:Create(NS,ti,{Transparency=1})
    local sst = TS:Create(NSS,ti,{Transparency=1})
    ft:Play()
    tt:Play()
    st:Play()
    sst:Play()
    ft.Completed:Connect(function() dp("Notification fade-out complete, destroying GUI") NG:Destroy() end)
end
dp("Notification GUI created, waiting 6 seconds before fade-out")
wait(6)
fadeOutNG()

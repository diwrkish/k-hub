local Players = game:GetService("Players")
local RS = game:GetService("ReplicatedStorage")
local TS = game:GetService("TweenService")
local UIS = game:GetService("UserInputService")
local LP = Players.LocalPlayer
local ESE = RS:WaitForChild("EquipSlapEvent")
local function dp(m) print("[InfYieldGui] "..m) end
local s,r = pcall(function() local pg = LP:WaitForChild("PlayerGui",5) if not pg then error("PlayerGui not found") end return pg end)
if not s then dp("Error: "..tostring(r)) return end
local PG = r
dp("PlayerGui found, creating GUIs")
local NG = Instance.new("ScreenGui")
NG.Name = "InfYieldNotifyGui"
NG.Parent = PG
NG.ResetOnSpawn = false
NG.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
NG.DisplayOrder = 101
local NF = Instance.new("Frame")
NF.Size = UDim2.new(0,300,0,100)
NF.Position = UDim2.new(0.5,-150,0.3,0)
NF.BackgroundColor3 = Color3.fromRGB(20,20,20)
NF.BackgroundTransparency = 0.2
NF.BorderSizePixel = 0
NF.Parent = NG
